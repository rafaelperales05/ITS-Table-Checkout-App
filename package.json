{
  "name": "its-table-checkout-app",
  "version": "1.0.0",
  "description": "University Table Checkout Management System - Proof of Concept",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "npm install && cd client && npm install && npm run build && cd ../server && npm install",
    "start": "cd server && npm start"
  },
  "keywords": [
    "table",
    "checkout",
    "management",
    "university",
    "react",
    "nodejs",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fuzzball": "^2.1.2",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pino": "^9.9.0",
    "postgres": "^3.4.7",
    "sequelize": "^6.35.2",
    "string-similarity": "^4.0.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
