{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install && cd server && npm install",
  "outputDirectory": "client/build",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}